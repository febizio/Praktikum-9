#include <iostream>

using namespace std;

int main()
{
    int i,angka,pangkat,hasil;

    i=1;
    hasil=1;
    cout<<"Masukkan Angka : ";
    cin>>angka;
    cout<<"Masukkan Pangkat : ";
    cin>>pangkat;
   do
   {
       hasil=hasil*angka;
       i++;
   }
   while (i<=pangkat);
   cout<<"Hasilnya : "<<hasil;
}
