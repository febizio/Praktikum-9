#include <iostream>

using namespace std;

int main()
{
    int i,angka,pangkat,hasil;

    i=1;
    hasil=1;
    cout<<"Masukkan Angka : ";
    cin>>angka;
    cout<<"Masukkan Pangkat : ";
    cin>>pangkat;
   while (i<= pangkat)
   {
       hasil=hasil*angka;
       i++;
   }
   cout<<"Hasilnya : "<<hasil;
}
